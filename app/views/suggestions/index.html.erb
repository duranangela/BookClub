<section>
  <% if current_user && current_admin? %>
    <h2>All suggestions: please add to database<h2>
    <% @suggestions.each do |suggestion| %>
      <%= suggestion.author %><br>
      <%= suggestion.title %><br>
      <%= suggestion.rating %><br>
      <%= suggestion.comment %><br>
      from: <%= suggestion.user.username %><br><br>
    <% end %>
  <% end %>
</section>
